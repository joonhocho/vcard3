{
  "name": "vcard3",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "description": "vCard 3.0 iOS compliant",
  "author": "Joon Ho Cho",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joonhocho/vcard3.git"
  },
  "bugs": {
    "url": "https://github.com/joonhocho/vcard3/issues"
  },
  "homepage": "https://github.com/joonhocho/vcard3#readme",
  "scripts": {
    "all": "npm run clean && npm run build:index && npm run format && npm run lint:fix && npm run build:all && npm run test",
    "build:all": "npm run build:types && npm run build:babel",
    "build:babel": "babel src --out-dir dist --extensions \".js,.jsx,.ts,.tsx\" --source-maps",
    "build:index": "tstf genIndex -o src/index.ts -w",
    "build:types": "tsc -p ./tsconfig.module.json --emitDeclarationOnly && tscpaths -p ./tsconfig.module.json -s ./src -o ./dist",
    "crawl": "ts-node scripts/crawl.ts",
    "crawl:flag": "ts-node scripts/flag.ts",
    "clean": "rm -rf ./dist ./coverage",
    "format": "prettier --write \"./*.{js,jsx,ts,tsx}\" \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:fix:dry": "eslint . --ext .ts,.tsx --fix-dry-run",
    "precommit": "npm run all",
    "prepublishOnly": "npm run all",
    "reinstall": "rm -rf ./node_modules ./package-lock.json ./yarn.lock && yarn",
    "start": "npm run test",
    "tsc": "tsc --noEmit",
    "tsc:watch": "npm run tsc -- --watch",
    "test": "NODE_ENV=test jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "test:coverage:report": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test:watch": "jest --watch"
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.22",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "@typescript-eslint/typescript-estree": "^3.6.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "jest": "^26.1.0",
    "prettier": "2.0.5",
    "react": "^16.13.1",
    "ts-jest": "^26.1.2",
    "ts-node": "^8.10.2",
    "tscpaths": "^0.0.9",
    "tsdef": "^0.0.13",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
